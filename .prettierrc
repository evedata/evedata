{
  "printWidth": 88,
  "singleQuote": false,
  "semi": true,
  "trailingComma": "es5",
  "proseWrap": "preserve",
  "overrides": [
    {
      "files": ["**/*.jsonc"],
      "options": {
        "trailingComma": "none"
      }
    }
  ],
  "plugins": ["prettier-plugin-packagejson", "prettier-plugin-toml"]
}
