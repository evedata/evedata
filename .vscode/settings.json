    "bonusing",
    "boto",
    "capitalisation",
    "capsuleers",
    "classmethod",
    "codegen",
    "commitizen",
    "compressibletypes",
    "createhome",
    "datetimez",
    "dbname",
    "dbuff",
    "dbuffs",
    "dgmattributes",
    "dockable",
    "dogmaeffectcategories",
    "dogmaunits",
    "dotenv",
    "dynamicitemattributes",
    "endfor",
    "endtest",
    "errmsg",
    "evedata",
    "evedatactl",
    "expertsystems",
    "fastapi",
    "filesizeformat",
    "fqcn",
    "FURB",
    "getdbt",
    "graphicmaterialsets",
    "hostvars",
    "htmlcov",
    "httpget",
    "industryactivities",
    "industryassemblylines",
    "industryinstallationtypes",
    "industrymodifiersources",
    "industrytargetfilters",
    "innodb",
    "jdcloud",
    "jinjafmt",
    "lineinfile",
    "lshw",
    "marimo",
    "mdstat",
    "memtotal",
    "minioadmin",
    "MOUNTPOINT",
    "mutaplasmid",
    "mutaplasmids",
    "mysqladmin",
    "nftables",
    "NOPASSWD",
    "noqa",
    "npccorps",
    "nvme",
    "passwordless",
    "pathlib",
    "pciutils",
    "Perflint",
    "PGPASSWORD",
    "psql",
    "pylint",
    "RDBMS",
    "refurb",
    "repackagedvolumes",
    "resolv",
    "retriable",
    "schoolmap",
    "skillplans",
    "skinmaterialnames",
    "skinmaterials",
    "smallint",
    "smartmontools",
    "sqlalchemy",
    "sqlcmd",
    "sqlfluff",
    "sqlfluffignore",
    "sqlfmt",
    "stackable",
    "stargate",
    "stargates",
    "stationstandingsrestrictions",
    "streamlit",
    "templater",
    "timestamptz",
    "tourn",
